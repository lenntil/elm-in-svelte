{
  "name": "elm-in-svelte3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 8943",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "elm:clean": "rm -rf ./static/elm",
    "elm:build:examples1": "cd ./src/lib/elm/examples1 && elm make src/Counter.elm src/TextField.elm --output=../../../../static/elm/elm.js --optimize",
    "elm:build:examples2": "node ./src/lib/elm/compileElm.cjs"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@types/elm": "^0.19.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module"
}
